<?xml version='1.0' encoding='utf-8'?>
<widget id="rf.gd.marinenko.urbansync" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UrbanSync</name>
    <description>
        Sync All your data over p2p storage (ipfs).
    </description>
    <author email="misha@marinenko.rf.gd" href="http://marinenko.rf.gd">
        Misha Marinenko
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.1.1" />
</widget>
